{
  "buildCommand": null,
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://api.sardis.sh/api/:path*" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
